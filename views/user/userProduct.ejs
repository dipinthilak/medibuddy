    <script>
        function addtoCart(productid)
      {
        $.ajax({
            method: 'POST',
            url: '/addtocart',
            data: { productid }, 
            success : function(res){
                // Handle the response
                if(res.notlogin){
                  Swal.fire(
                              'You should login first'
                            ).then(function(){
                              window.location.href="/userSignin";

                            });
                }
                else if(res.toCart){
                  Swal.fire(
                              'Item added to cart'
                            );
                }
                else{
                  Swal.fire(
                              'Item not added to cart'
                            );
                }
            }
        });
      }


    </script>
