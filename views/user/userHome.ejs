    <script>
      function addtoWishlist(productid) {
        toastr.options = {
          closeButton: false,
          newestOnTop: false,
          progressBar: true,
          positionClass: "toast-bottom-right",
          preventDuplicates: false,
          onclick: null,
          showDuration: "300",
          hideDuration: "1000",
          timeOut: "5000",
          extendedTimeOut: "1000",
          showEasing: "swing",
          hideEasing: "linear",
          showMethod: "fadeIn",
          hideMethod: "fadeOut",
        };
        $.ajax({
          method: "POST",
          url: "/addtowishlist",
          data: { productid },
          success: function (res) {
            // Handle the response
            if (res.notlogin) {
              Swal.fire("You should login first").then(function () {
                window.location.href = "/userSignin";
              });
            } else if (res.exsting) {
              $(document).ready(function onDocumentReady() {
                toastr.error("Item already in WISHLIST");
              });
            } else if (res.towishlist) {
              if (typeof res.wlsize == "number") {
                const wlicon = document.getElementById("wlicon");
                wlicon.innerHTML = res.wlsize;
              }
              $(document).ready(function onDocumentReady() {
                toastr.success("Item added to WISHLIST");
              });
            } else {
              $(document).ready(function onDocumentReady() {
                toastr.error("Item not added to WISHLIST");
              });
            }
          },
        });
      }

      function userCart() {
        console.log("userCart");
        $.ajax({
          method: "GET",
          url: "/cartOpen",
          success: function (res) {
            console.log("cart open check");
            if (res.notlogin) {
              console.log("no login");
              Swal.fire("you should login first").then(function () {
                window.location.href = "/userSignin";
              });
            } else if (res.userlogin) {
              window.location.href = "/userCart";
            }
          },
        });
      }

      function userWlist() {
        console.log("userWlist");
        $.ajax({
          method: "GET",
          url: "/wsOpen",
          success: function (res) {
            console.log("ws open check");
            if (res.notlogin) {
              console.log("no login");
              Swal.fire("you should login first").then(function () {
                window.location.href = "/userSignin";
              });
            } else if (res.userlogin) {
              window.location.href = "/userWishlist";
            }
          },
        });
      }

      function addtoCart(productid) {
        $.ajax({
          method: "POST",
          url: "/addtoCart",
          data: { productid },
          success: function (res) {
            toastr.options = {
              closeButton: false,
              newestOnTop: false,
              progressBar: true,
              positionClass: "toast-bottom-right",
              preventDuplicates: false,
              onclick: null,
              showDuration: "300",
              hideDuration: "1000",
              timeOut: "5000",
              extendedTimeOut: "1000",
              showEasing: "swing",
              hideEasing: "linear",
              showMethod: "fadeIn",
              hideMethod: "fadeOut",
            };
            // Handle the response
            if (res.notlogin) {
              Swal.fire("You should login first").then(function () {
                window.location.href = "/userSignin";
              });
            } else if (res.toCart) {
              if (typeof res.cartcount == "number") {
                const cartl = document.getElementById("clength");
                cartl.innerHTML = res.cartcount;
              }

              $(document).ready(function onDocumentReady() {
                toastr.success("'Item added to cart'");
              });
            } else {
              $(document).ready(function onDocumentReady() {
                toastr.error("'Item not added to cart'");
              });
            }
          },
        });
      }
    </script>
